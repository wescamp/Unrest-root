msgstr "Re guerriero degli elfi"

#: Unrest/units/Elvish_Warrior_King.cfg:17
#: Unrest/units/Elvish_Warrior_Prince.cfg:18
msgid "leadership"
msgstr "comando"

#: Unrest/units/Elvish_Warrior_King.cfg:27
#: Unrest/units/Elvish_Warrior_Prince.cfg:28
msgid "sword"
msgstr "spada"

#: Unrest/units/Elvish_Warrior_King.cfg:28
#: Unrest/units/Elvish_Warrior_Prince.cfg:29 Unrest/units/Nightwing_Bat.cfg:22
msgid "blade"
msgstr "lama"

#: Unrest/units/Elvish_Warrior_King.cfg:38
#: Unrest/units/Elvish_Warrior_Prince.cfg:39
msgid "fireball"
msgstr "palla di fuoco"

#: Unrest/units/Elvish_Warrior_King.cfg:39
#: Unrest/units/Elvish_Warrior_Prince.cfg:40 Unrest/units/Forest_Fire.cfg:36
msgid "fire"
msgstr "fuoco"

#: Unrest/units/Elvish_Warrior_King.cfg:43
msgid "magical"
msgstr "magico"

#: Unrest/units/Elvish_Warrior_Prince.cfg:4
msgid "Elvish Warrior Prince"
msgstr "Principe guerriero degli elfi"

#: Unrest/units/Forest_Fire.cfg:3
msgid "Forest Fire"
msgstr "Fuoco della foresta"

#: Unrest/units/Forest_Fire.cfg:35
msgid "burn"
msgstr "brucia"

#: Unrest/units/Nightwing_Bat.cfg:4
msgid "Nightwing Bat"
msgstr "Pipistrello notturno"

#: Unrest/units/Nightwing_Bat.cfg:17
msgid "nightstalk"
msgstr "Assalitore notturno"

#: Unrest/units/Nightwing_Bat.cfg:18
msgid "They're invisible at night and they suck your blood! Fear them!"
msgstr "Sono invisibili di notte e succhiano il sangue! Spaventosi!"

#: Unrest/units/Nightwing_Bat.cfg:21
msgid "fangs"
msgstr "zanne"

#: Unrest/units/Nightwing_Bat.cfg:26
msgid "drain"
msgstr "risucchio"
