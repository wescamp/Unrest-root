msgstr "espasa"

#: Unrest/units/Elvish_Warrior_King.cfg:29 Unrest/units/Nightwing_Bat.cfg:22
#: Unrest/units/Elvish_Warrior_Prince.cfg:30
msgid "blade"
msgstr "talla"

#: Unrest/units/Elvish_Warrior_King.cfg:39
#: Unrest/units/Elvish_Warrior_Prince.cfg:40
msgid "fireball"
msgstr "bola de foc"

#: Unrest/units/Elvish_Warrior_King.cfg:40 Unrest/units/Forest_Fire.cfg:36
#: Unrest/units/Elvish_Warrior_Prince.cfg:41
msgid "fire"
msgstr "foc"

#: Unrest/units/Elvish_Warrior_King.cfg:44
msgid "magical"
msgstr "màgic"

#: Unrest/units/Elvish_Warrior_Prince.cfg:3
msgid "Elvish Warrior Prince"
msgstr "Príncep Guerrer Elf"

#: Unrest/units/Forest_Fire.cfg:3
msgid "Forest Fire"
msgstr "Incendi Forestal"

#: Unrest/units/Forest_Fire.cfg:35
msgid "burn"
msgstr "cremar"

#: Unrest/units/Nightwing_Bat.cfg:4
msgid "Nightwing Bat"
msgstr "Rat-penat atziac"

#: Unrest/units/Nightwing_Bat.cfg:17
msgid "nightstalk"
msgstr "sigil"

#: Unrest/units/Nightwing_Bat.cfg:18
msgid "They're invisible at night and they suck your blood! Fear them!"
msgstr ""
"Son invisibles de nit i et xuclen la sang! Més val que els tingues por!"

#: Unrest/units/Nightwing_Bat.cfg:21
msgid "fangs"
msgstr "ullals"

#: Unrest/units/Nightwing_Bat.cfg:26
msgid "drain"
msgstr "drena"
